<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.bhavikpatel.guni">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".CalanderActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_calander"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".CreaditsActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_creadits"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".MarksActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_marks"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".SylActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_syllabus"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".ResultActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_result"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".AttendenceActivity"
            android:parentActivityName=".MainActivity"
            android:label="@string/app_attendence"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".DateActivity"
            android:parentActivityName=".MainActivity"
            android:label="Date"
            android:theme="@style/AppTheme">
        </activity>
        <activity
            android:name=".AttDesActivity"
            android:parentActivityName=".MainActivity"
            android:label="Date"
            android:theme="@style/AppTheme">
        </activity>
    </application>

</manifest>